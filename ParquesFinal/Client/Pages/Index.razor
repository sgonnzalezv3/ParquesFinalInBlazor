@page "/"
@attribute [Authorize]
@attribute [AllowAnonymous]


    <h1 class="principal mb-0 my-0">PARQUES NATURALES</h1>
    <section class="container-fluid mb-0 my-0 px-xs-0">
        <div class="row">
